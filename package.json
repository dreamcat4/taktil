{
  "name": "taktil",
  "description": "Taktil is a lightweight control surface scripting framework for Bitwig Studio that encourages rapid development and community code reuse.",
  "version": "0.5.1",
  "repository": "taktiljs/taktil",
  "author": "Joseph Larson",
  "tags": [
    "bitwig",
    "control surface script",
    "framework",
    "typescript"
  ],
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.16",
    "@types/sinon": "^2.2.1",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.6",
    "merge2": "^1.0.3",
    "mocha": "^3.3.0",
    "prettier": "^1.4.2",
    "sinon": "^2.2.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-config-prettier": "^1.1.0",
    "tsutils": "^1.6.0",
    "typed-bitwig-api": "^2.0.0",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "start": "gulp watch",
    "build": "gulp build",
    "clean": "gulp clean",
    "test": "mocha -r ts-node/register -r ./env $(find . ! -path './.git*' ! -path './node_modules*' -path '*.spec.ts')",
    "lint": "tslint -c tslint.json $(find . ! -path './.git*' ! -path './node_modules*' -path '*.ts') || true",
    "format": "prettier --parser typescript --print-width 100 --tab-width 4 --single-quote --trailing-comma=es5 --write $(find . ! -path './.git*' ! -path './node_modules*' -path '*.ts')",
    "preversion": "npm test && gulp build",
    "postversion": "gulp clean",
    "prepack": "npm test && gulp build",
    "postpack": "gulp clean"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/taktil/taktiljs/issues"
  },
  "homepage": "https://github.com/taktil/taktiljs#readme"
}